	postLogin = function(){
		if($('#EMAIL_ID').val() == ""){
			errorElem.push("EMAIL_ID");
		}
		if($('#PASSWORD').val() == ""){
			errorElem.push("PASSWORD");
		}
		if($('#logincaptcha').val() == ""){
			errorElem.push("logincaptcha");
		}
		if(errorElem.length > 0){
			errorBlock();
			$('.modal-dialog').find('.alertMsg').html('<div class="alert alert-danger"><button aria-hidden="true" data-dismiss="alert" class="close" type="button">×</button>Please enter data for required fields!</div>');
			return false;
			}else{
			$.ajax({
				type: "POST",
				url: base_url+"checkLogin",
				data: $('#login').serialize(),
				dataType:'json',
				beforeSend: function(){
					$('#loader').show();
					$('.modal-dialog').find('.alertMsg').html("");
				},
